<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="jiangmh.demo.crm.workbench.dao.ContactsRemarkDao">
	<insert id="save" parameterType="list">
        insert into tbl_contacts_remark(
        id,
        noteContent,
        createBy,
        createTime,
        editFlag,
        contactsId
        )values
        <foreach collection="list" item="item" separator=",">
            (
            #{item.id},
            #{item.noteContent},
            #{item.createBy},
            #{item.createTime},
            #{item.editFlag},
            #{item.contactsId}
            )
        </foreach>
    </insert>
  	
</mapper>

































